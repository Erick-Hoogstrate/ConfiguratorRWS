group $name$: //$type$
  automaton Sensor:
    uncontrollable u_on, u_off;
    location On:
      edge u_off goto Off;
    location Off:
      initial; 
      edge u_on  goto On;
  end

  alg bool On  = Sensor.On;
  alg bool Off = Sensor.Off;

  svgout id "$name$.background" attr "fill" value if On: "yellow" else "grey" end file $filename$;
  svgin id "$name$" event if Sensor.On: Sensor.u_off else Sensor.u_on end file $filename$;
end
