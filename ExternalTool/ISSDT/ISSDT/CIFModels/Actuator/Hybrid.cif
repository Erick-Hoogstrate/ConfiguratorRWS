group $name$: //$type$
  controllable c_on, c_off;
  automaton Actuator:
    location On:
      edge c_off goto Off;
    location Off:
      initial; 
      edge c_on  goto On;
  end
  alg bool On  = Actuator.On;
  alg bool Off = Actuator.Off;

  svgout id "$name$.background" attr "fill" value if On: "yellow" else "grey" end file $filename$;
end